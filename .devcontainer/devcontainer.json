{
    "name": "racket-playground",
    "dockerComposeFile": ["docker-compose.yml"],
    "service": "workspace",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "EditorConfig.EditorConfig",
                "shardulm94.trailing-spaces",
                "evzen-wybitul.magic-racket",
                "ms-toolsai.jupyter"
            ]
        }
    },
    "remoteUser": "vscode",
    "postStartCommand": "raco iracket install"
}
